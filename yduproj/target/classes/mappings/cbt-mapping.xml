<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
                        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

                        
<mapper namespace="com.ydu.biz.cbt.impl.CBTMapper"> 

	<select id="getBoard" resultType="cbtList">
        <![CDATA[
        SELECT * 
        FROM BOARD
        WHERE SEQ = #{seq}
        ]]>
    </select>
	
	<select id="getAllCBT" resultType="" parameterType="">
		SELECT *
		FROM CBT_LIST
		ORDER BY CBT_CODE
	</select>
	
	<select id="getMyCBT" resultType="map" >
		SELECT student_code as "studentCode" 
		FROM CBT_LIST
		WHERE 
	</select>
	
</mapper>
